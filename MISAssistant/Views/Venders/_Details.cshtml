@model MISAssistant.Models.vender
@{
    ViewBag.Title = "Details";
}
<!-- Modal content-->
<div class="modal-header modal-header-primary">
    <button type="button" class="close" aria-label="Close" data-dismiss="modal">&times;</button>
    <h4 class="modal-title">廠商［詳細］</h4>
</div>
<div id="detailBody" class="modal-body">
    <dl class="dl-horizontal">
        <dt style="height:26px">
            @Html.DisplayNameFor(model => model.company)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.company)
        </dd>

        <dt style="height:26px">
            @Html.DisplayNameFor(model => model.contact)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.contact)
        </dd>

        <dt style="height:26px">
            @Html.DisplayNameFor(model => model.title)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.title)
        </dd>

        <dt style="height:26px">
            @Html.DisplayNameFor(model => model.email)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.email)
        </dd>

        <dt style="height:26px">
            @Html.DisplayNameFor(model => model.tel_office)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.tel_office)
        </dd>

        <dt style="height:26px">
            @Html.DisplayNameFor(model => model.tel_mobile)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.tel_mobile)
        </dd>

        <dt style="height:26px">
            @Html.DisplayNameFor(model => model.fax)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.fax)
        </dd>

        <dt style="height:26px">
            @Html.DisplayNameFor(model => model.quick_no)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.quick_no)
        </dd>

        <dt style="height:26px">
            @Html.DisplayNameFor(model => model.note)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.note)
        </dd>
    </dl>
</div>
<div class="modal-footer modal-footer-primary">
    @Html.ActionLink("編輯", "_Edit", new { id = @Html.DisplayFor(model => model.id) }, new { @class = "btn btn-default", data_target = "#editModal", data_toggle = "modal" })
    @Html.ActionLink("返回", "Index", new { company = @Html.DisplayFor(model => model.company) }, new { @class = "btn btn-default" })
</div>
